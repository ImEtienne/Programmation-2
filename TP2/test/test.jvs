void main() {
    int n = 5;
    for (int i = 1; i < n; i = i + 1) {
        for (int j = n - 1; j > i; j = j - 1) {
            print(" ");
        }
        for (int k = 0; k < i; k = k + 1) {
            print("*");
        }
        for (int j = i - 1; j > 0; j = j - 1) {
            print("*");
        }
        println("");
    }
}

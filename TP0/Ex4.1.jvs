void main() {
    clear();
    int[] tab = new int[5];
    int n = 1;
    int c = 0;
    int arret = 0;
    while (arret == 0) {
        if (n >= 1 && n <= 5) {
            n = readInt();
            tab[n - 1] = tab[n - 1] + 1;
            c = c + 1;
            if (tab[n - 1] == 3) {
                arret = 1;
            }
        }
    }
    print(c);
}
